{
  "name": "avalon-item-generator",
  "version": "1.0.0",
  "description": "Générateur d'objet pour Avalon",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve",
    "start": "npm run build:prod && npm run electron",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "pack": "npm run build:prod && electron-builder --dir",
    "dist": "npm run build:prod && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "deploy": "copyfiles -f dist/angular-electron/**/* ../avalon-public"
  },
  "build": {
    "appId": "fr.5d6corp.avalon-item-generator",
    "productName": "Avalon item generator",
    "files": [
      "dist/angular-electron/**/*",
      "main.js"
    ],
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "portable",
      "icon": "src/favicon.png"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.1.7",
    "@angular/localize": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "bootstrap": "^4.4.0",
    "lodash": "^4.17.15",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.152",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.3.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
